{
  "name": "trello",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./ --fix",
    "format:fix": "pretty-quick --staged",
    "wdio": "wdio run ./wdio.conf.js",
    "test:login": "wdio run ./wdio.conf.js --suite login",
    "test:logout": "wdio run ./wdio.conf.js --suite logout",
    "test:create_boards": "wdio run ./wdio.conf.js --suite create_boards",
    "test:create_card": "wdio run ./wdio.conf.js --suite create_card",
    "test:search_card_board": "wdio run ./wdio.conf.js --suite search_card_board",
    "test:dropbox": "newman run dropbox.collection.json -e dropbox.environment.json --delay-request 1000",
    "test:cucumber:login": "wdio run ./wdio.conf.js --cucumberOpts.tagExpression=@login",
    "test:cucumber:logout": "wdio run ./wdio.conf.js --cucumberOpts.tagExpression=@logout",
    "test:cucumber:create_boards": "wdio run ./wdio.conf.js --cucumberOpts.tagExpression=@create_boards",
    "test:cucumber:create_card": "wdio run ./wdio.conf.js --cucumberOpts.tagExpression=@create_card",
    "test:cucumber:search_card_board": "wdio run ./wdio.conf.js --cucumberOpts.tagExpression=@search_card_board",
    "report:generate": "allure generate allure-results/",
    "report:open": "allure open",
    "prepare": "husky"
  },
  "keywords": [
    "Trello",
    "Test automation"
  ],
  "author": "Gergő Mátyus",
  "license": "ISC",
  "devDependencies": {
    "@wdio/allure-reporter": "^8.32.4",
    "@wdio/cli": "^8.32.3",
    "@wdio/cucumber-framework": "^8.35.0",
    "@wdio/local-runner": "^8.32.3",
    "@wdio/mocha-framework": "^8.32.3",
    "@wdio/spec-reporter": "^8.32.2",
    "eslint": "^8.57.0",
    "eslint-config-google": "^0.14.0",
    "husky": "^9.0.11",
    "newman": "^6.1.1",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "pretty-quick": "^4.0.0"
  }
}
